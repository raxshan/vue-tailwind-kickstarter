{
  "name": "vue-tailwind-kickstart",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run build:postcss && vue-cli-service serve",
    "build:postcss": "postcss src/css/tailwind.css -o public/css/grid.css",
    "build": "npm run build:postcss && vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "apollo-cache-persist": "^0.1.1",
    "autoprefixer": "^9.8.4",
    "concurrently": "^5.1.0",
    "core-js": "^3.6.5",
    "jwt-decode": "^2.2.0",
    "postcss-cli": "^7.1.0",
    "propper": "^1.0.3",
    "tailwindcss": "^1.2.0",
    "vue": "^2.6.11",
    "vue-apollo": "^3.0.0-beta.11",
    "vue-router": "^3.1.5"
  },
  "devDependencies": {
    "@tailwindcss/custom-forms": "^0.2.1",
    "@types/jest": "^24.0.19",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-service": "~4.4.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-graphql": "^3.1.1",
    "eslint-plugin-vue": "^6.1.2",
    "graphql-tag": "^2.9.0",
    "typescript": "~3.7.5",
    "vue-cli-plugin-apollo": "^0.21.3",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
